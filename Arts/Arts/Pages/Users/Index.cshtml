@page
@model Arts.Pages.Users.IndexModel
@{
    ViewData["Title"] = "List users";
}

<br />
<h2>List of users</h2>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Username</th>
            <th>Email</th>
            <th>Password</th>
            <th>Is Admin</th>
            <th>Create At</th>
        </tr>
    </thead>
    <tbody>
       @foreach (var item in Model.ListUsers)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Username</td>
                <td>@item.Useremail</td>
                <td>@item.Userpassword</td>
                <td>@item.IsAdmin</td>
                <td>@item.CreateAt</td>
            </tr>
        }
    </tbody>
</table>
